			<column name="FD_CREATE_DATE"       			type="DATETIME"              	 			   	remarks="创建时间"></column>
			<column name="FD_LAST_MODIFYDATE"   			type="DATETIME"              	 			   	remarks="更新时间"></column>
        </createTable>
       
        	
