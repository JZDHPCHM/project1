			</column>
            <column name="FD_SCALE_CODE"				type="java.sql.Types.VARCHAR(50)"				remarks="标尺编号"></column>
			<column name="FD_SCALE_NAME"				type="java.sql.Types.VARCHAR(254)"				remarks="标尺名称"></column>
