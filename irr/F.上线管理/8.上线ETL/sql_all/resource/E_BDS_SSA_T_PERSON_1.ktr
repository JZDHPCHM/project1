    <sql>select distinct  &#xd;&#xa;      --c.dateid, --&#x7edf;&#x8ba1;&#x65f6;&#x95f4;&#xd;&#xa;      --yu.innerorgcode, --&#x7ba1;&#x7406;&#x673a;&#x6784;&#xd;&#xa;      --r.rankid,&#xd;&#xa;      --re.tid AS tid,&#xd;&#xa;      --0 as NewAddPsnNum, --&#x65b0;&#x5165;&#x53f8;&#x4eba;&#x529b;&#xd;&#xa;      --0 as DivisionPsnNum, --&#x79bb;&#x53f8;&#x4eba;&#x529b;&#xd;&#xa;       Y.PID , --&#x671f;&#x672b;&#x4eba;&#x529b;&#xd;&#xa;       Y.PNAME,&#xd;&#xa;       Y.PSNTYPENO,&#xd;&#xa;       Y.DID,&#xd;&#xa;       y.workdate,&#xd;&#xa;       y.dimissdate,&#xd;&#xa;       Y.STATUS&#xd;&#xa;      --0 as ActivPsnNum,&#xd;&#xa;      -- y.add_bkcod&#xd;&#xa;  from bidw.D_PSNNATURE y&#xd;&#xa; inner join BIDM.d_date c&#xd;&#xa;    on y.dimissdate &#x3e; c.dateid&#xd;&#xa;   and y.workdate &#x3c;&#x3d; c.dateid&#xd;&#xa; inner join &#x28;SELECT to_char&#x28;sysdate,&#x27;YYYYMMDD&#x27;&#x29; AS SE_DATE FROM DUAL&#x29; et&#xd;&#xa;    on c.DATEID between&#xd;&#xa;       to_char&#x28;add_months&#x28;to_date&#x28;ET.SE_DATE, &#x27;YYYYMMDD&#x27;&#x29;, -2&#x29;, &#x27;yyyymm&#x27;&#x29; &#x7c;&#x7c; &#x27;01&#x27; and&#xd;&#xa;       et.SE_DATE&#xd;&#xa;  left join BIDW.RE_psn_Group re&#xd;&#xa;    on Y.PID &#x3d; RE.PID&#xd;&#xa;   AND RE.STARTDATE &#x3c;&#x3d; c.dateid&#xd;&#xa;   AND RE.ENDDATE &#x3e; c.dateid, BIDM.d_orgmap YU, bidw.d_rank r&#xd;&#xa; where substr&#x28;y.Did, 4, 3&#x29; &#x3d; &#x27;001&#x27;&#xd;&#xa;   and r.pid &#x3d; y.pid&#xd;&#xa;   and c.dateid between r.startdate and r.enddate&#xd;&#xa;   and y.chnlcode &#x3d; &#x27;FC&#x27;&#xd;&#xa;   and SUBSTR&#x28;y.DID, 1, 3&#x29; &#x3d; YU.ORIGORGCODE&#xd;&#xa;   and y.psntypeno &#x21;&#x3d; &#x27;D&#x27;&#xd;&#xa;   and c.dateid&#x3d;CASE WHEN TO_CHAR&#x28;SYSDATE,&#x27;MM&#x27;&#x29; IN &#x28;&#x27;01&#x27;, &#x27;02&#x27;, &#x27;03&#x27;&#x29; THEN TO_CHAR&#x28;TRUNC&#x28;SYSDATE,&#x27;yyyy&#x27;&#x29;-1,&#x27;YYYY&#x27;&#x29; &#x7c;&#x7c; &#x27;1231&#x27;&#xd;&#xa;     WHEN TO_CHAR&#x28;SYSDATE,&#x27;MM&#x27;&#x29; IN &#x28;&#x27;04&#x27;, &#x27;05&#x27;, &#x27;06&#x27;&#x29; THEN TO_CHAR&#x28;SYSDATE, &#x27;YYYY&#x27;&#x29; &#x7c;&#x7c; &#x27;0331&#x27;&#xd;&#xa;     WHEN TO_CHAR&#x28;SYSDATE,&#x27;MM&#x27;&#x29; IN &#x28;&#x27;07&#x27;, &#x27;08&#x27;, &#x27;09&#x27;&#x29; THEN TO_CHAR&#x28;SYSDATE, &#x27;YYYY&#x27;&#x29; &#x7c;&#x7c; &#x27;0630&#x27;&#xd;&#xa;     WHEN TO_CHAR&#x28;SYSDATE,&#x27;MM&#x27;&#x29; IN &#x28;&#x27;10&#x27;, &#x27;11&#x27;, &#x27;12&#x27;&#x29; THEN TO_CHAR&#x28;SYSDATE, &#x27;YYYY&#x27;&#x29; &#x7c;&#x7c; &#x27;0930&#x27; &#xd;&#xa;	   END</sql>
