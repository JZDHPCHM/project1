<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="T_IRR_TOTALSCORE_RESULT" author="fc">
        <createTable tableName="T_IRR_TOTALSCORE_RESULT" remarks="总分多维度结果表">
			<column name="ID"					type="java.sql.Types.VARCHAR(36)"		remarks="主键ID">
				<constraints primaryKey="true"/>
			</column>
			<column name="PROJ_ID"       			type="VARCHAR(36)"      	 	remarks="评估项目ID"></column>
			<column name="PROJ_NAME" 				type="VARCHAR(300)" 			remarks="评估项目名称"></column>
			<column name="PROJ_CODE"      			type="VARCHAR(50)"      	 	remarks="评估项目编码"></column>
			<column name="PROJ_RESULT"    			type="VARCHAR(300)"      		remarks="评估项目结果"></column>
			<column name="RESULT_FLAG"    			type="VARCHAR(30)"      		remarks="结果标识"></column>
			<column name="TASK_ID"     				type="VARCHAR(36)"      	 	remarks="任务ID"></column>
			<column name="TASK_BATCH"     			type="VARCHAR(10)"      	 	remarks="任务期次"></column>
			<column name="ORG_ID"     				type="VARCHAR(36)"      	 	remarks="机构ID"></column>
			<column name="DATA_BODY"      			type="VARCHAR(50)"      		remarks="主体编码"></column>
			<column name="FD_CREATOR"           	type="VARCHAR(255)"  			remarks="创建人"></column>
			<column name="FD_CREATE_DATE"       	type="DATETIME"                 remarks="创建日期"></column>
			<column name="FD_LAST_MODIFIER"     	type="VARCHAR(50)"  			remarks="最后修改人"></column>
			<column name="FD_LAST_MODIFYDATE"   	type="DATETIME"                 remarks="最后修改日期"></column>
        </createTable>
    </changeSet>
</databaseChangeLog>
